<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.4//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.mybatis.mapper.StudentMapper">

    <!-- 查询全部学生数量 -->
    <select id="selectAllStudentCount" resultType="int">
        SELECT count(1)
        FROM student
    </select>
	<!-- 查询全部学生信息 -->
    <select id="selectAllStudent" resultType="com.app.entity.Student">
        SELECT *
        FROM student
    </select>
   	
</mapper>